<% if Photo.all.empty? %>
  No photos yet
<% else %>
  <% @photos.each do |photo| %>
    <div class="well">
      <ul class="media-list"
        <li class="media">
          <div class="media-left">
            <a href="#">
              <%= image_tag photo.image.url(:medium), class: "media-object" %>
            </a>
          </div>
          <div class="media-body">
            <h4 class="media-heading">By <%= photo.user.username %></h4>
            Description: <%= photo.description %>
          </div>
        </li>
      </ul>
    </div>
  <% end %>
<% end %>